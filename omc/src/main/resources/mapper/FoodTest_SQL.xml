<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="foodtest">

	<!-- [testResultSet] 신상품 리스트 -->
	<select id="testResultSet" parameterType="hashmap" resultType="hashmap"> 
      <![CDATA[
      SELECT *
      FROM FOODTEST
      WHERE FT_TITLE = #{GD_CATEGORY}
      ]]>
   </select>

</mapper>